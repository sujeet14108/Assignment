            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Store_internal" />
        <activity android:name=".Display_records" />
        <activity android:name=".Store_external"></activity>
    </application>

</manifest>